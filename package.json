{
  "name": "vite-react-ts-minimal-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss}\" --write",
    "compile": "tsc",
    "preview": "vite preview",
    "prepare": "husky install"
