    "@emotion/react": "11",
    "@emotion/styled": "11",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.27.2",
    "framer-motion": "6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "6"
